<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>CadCliente</servlet-name>
        <servlet-class>controle.CadCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadProdutos</servlet-name>
        <servlet-class>controle.CadProdutos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadFunc</servlet-name>
        <servlet-class>controle.CadFunc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsCliente</servlet-name>
        <servlet-class>controle.ConsCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConCliente</servlet-name>
        <servlet-class>controle.ConCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teste</servlet-name>
        <servlet-class>controle.Teste</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controle.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deslogar</servlet-name>
        <servlet-class>controle.deslogar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadCliente</servlet-name>
        <url-pattern>/CadCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadProdutos</servlet-name>
        <url-pattern>/CadProdutos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadFunc</servlet-name>
        <url-pattern>/CadFunc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsCliente</servlet-name>
        <url-pattern>/ConsCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConCliente</servlet-name>
        <url-pattern>/ConCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teste</servlet-name>
        <url-pattern>/Teste</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deslogar</servlet-name>
        <url-pattern>/deslogar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
