package aula2;

public class CondicaoIf {

	public static void main(String[] args) {
		/*
		 * comando condicional if - else
		 * 
		 * if(condiçao){
		 * 	comandos;
		 * }else{
		 * 	comandos;
		 * }
		 * 
		 * */
		int n1=10,n2=20;
		
		if(n1 > n2){
			System.out.println("n1 maior que n2");
		}else{
			System.out.println("n2 maior que n1");
		}
	}

}
