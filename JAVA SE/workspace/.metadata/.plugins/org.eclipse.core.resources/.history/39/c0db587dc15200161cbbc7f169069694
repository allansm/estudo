package aula2;

public class Ex1 {

	public static void main(String[] args) {
		int a=6,b=6,c=5;
		int maior = a;
		int menor = a;
		if(maior < b){
			maior = b;
		}else{
			menor = b;
		}
		if(maior < c){
			maior = c;
		}else{
			menor = c;
		}
		System.out.println("o maior numero foi o "+maior+" e o menor foi "+menor);
	}

}
